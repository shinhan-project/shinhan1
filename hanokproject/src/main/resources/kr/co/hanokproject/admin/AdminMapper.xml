<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="kr.co.hanokproject.admin.AdminMapper">
    <select id="login" parameterType="map" resultType="kr.co.hanokproject.admin.AdminVO">
        SELECT * FROM USERS_ADMIN WHERE
        admin_id=#{admin_id} AND admin_pwd = MD5(#{admin_pwd})
    </select>
</mapper>
